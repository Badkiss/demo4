<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="470.0" prefWidth="510.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.12" fx:controller="org.example.demo4.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <Label text="Nombre">
            <graphic>
               <TextField fx:id="nombre" />
            </graphic>
         </Label>
      </children>
   </HBox>
   <Label text="IP" />
   <TextField fx:id="IP" />
   <Label text="Puerto" />
   <TextField fx:id="puerto" />
    <Button fx:id="start" onAction="#onStart" text="Conectar" />
   <TextField />
   <Button mnemonicParsing="false" text="Enviar" />
   <VBox prefHeight="200.0" prefWidth="100.0" />
</VBox>
